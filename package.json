{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently -n netlify,proxify 'yarn dev:netlify' 'yarn dev:proxify'",
    "dev:netlify": "cross-env NODE_ENV=development netlify dev",
    "dev:proxify": "node proxify.js",
    "start": "cross-env NODE_ENV=production netlify dev",
    "fmt": "yarn commands:prettier --write",
    "lint": "yarn commands:eslint",
    "commands:prettier": "prettier **/*.{js,ts,tsx,json,md}",
    "commands:eslint": "eslint *.js *.ts app",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
